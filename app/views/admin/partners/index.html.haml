%h1
  Đối tác
  .pull-right
    = link_to new_admin_partner_path, "data-keyboard" => false, "data-backdrop" => "static", "data-toggle"=>"modal", "data-target"=>"#foxyModal", class: 'btn btn-sign-up-now-without-bg btn-xs' do
      %i.fa.fa-filter
    = link_to new_admin_partner_path, "data-keyboard" => false, "data-backdrop" => "static", "data-toggle"=>"modal", "data-target"=>"#foxyModal", class: 'btn btn-sign-up-now-without-bg btn-xs' do
      %i.fa.fa-cloud-upload

    = link_to new_admin_partner_path, "data-keyboard" => false, "data-backdrop" => "static", "data-toggle"=>"modal", "data-target"=>"#foxyModal", class: 'btn btn-sign-up-now-without-bg btn-xs' do
      %i.fa.fa-cloud-download

    = link_to new_admin_partner_path, "data-keyboard" => false, "data-backdrop" => "static", "data-toggle"=>"modal", "data-target"=>"#foxyModal", class: 'btn btn-sign-up-now btn-xs' do
      %i.fa.fa-plus
      Thêm mới
%hr
.row
  .col-md-12
    %table.table
      %thead
        %tr
          %th #
          %th Họ và tên
          %th Email
          %th Loại đối tác

          %th.text-right
      %tbody
        - @partners.each do |partner|
          = render 'partner', partner: partner
